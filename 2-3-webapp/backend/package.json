    "mysql": "github:mysqljs/mysql",
    "promise-mysql": "^3.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.1.0",
    "mocha": "^5.2.0",
